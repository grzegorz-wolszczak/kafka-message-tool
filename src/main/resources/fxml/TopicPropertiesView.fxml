                                    <TableColumn fx:id="beginOffsetColumn" prefWidth="75.0" text="Begin offset" />
                                    <TableColumn fx:id="endOffsetColumn" prefWidth="75.0" text="End offset" />
                                </columns>
                              </TableView>
                           </children></AnchorPane>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
            <Button fx:id="closeButton" mnemonicParsing="false" onAction="#closeOnAction" text="Close" GridPane.rowIndex="2" />
            <Label fx:id="titleLabel" text="Label">
